{
  //-Choisissez combien de caractère peut contenir l'extrait de la chronique en page d'accueil :
  "MaxDescriptionTitre": 200,

  //-Choisissez combien de titres chroniqués doivent apparaître sur la page d'accueil :
  "NbDernierTitreChronique": 3,

  //-Choisissez sur combien de mois les 3 titres les plus populaires sont sélectionnés :
  "NbMoisDernierTitrePopulaire": 3,

  //-Choisissez la création ou non d'une base de données :
  //Les données sont stockées sur une BDD : Decommenter "Repository": "db" et Commenter "Repository": "Local"
  //Les données ne sont pas stockées sur une BDD : Decommenter "Repository": "Local"  et Commenter "Repository": "db"

  //"Repository": "Local",
  "Repository": "db",

  //-Aucun choix possible sur le SGBD :
  //Dans l'environnement de développement, la BDD est stockée grâce à SQLite :
  "SGBD": "SQLite",

  //Dans l'environnement de production, la BDD est stockée grâce à Postgre.

  //-Choisissez d'où proviennent les données :
  //Pour de fausses données : Decommenter "Seeder": "" et Commenter "Seeder": "Spotify"
  //Pour des données provenant de Spotify : Decommenter "Seeder": "Spotify"  et Commenter "Seeder": "" si vous voulez avoir 

  //"Seeder": "Spotify",
  "Seeder": "",

  //Chaine de connexion à Postgr complétée automatiquement en prod :
  "ConnectionStrings": {
    "PostgresConnection": ""

  },
  //Chaine de connexion à Spotify complétée automatiquement en prod :
  "Spotify": {
    "ClientId": "",
    "ClientSecret": ""
  }
}
